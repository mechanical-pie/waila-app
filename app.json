{
  "expo": {
    "name": "WAILA",
    "slug": "WAILA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/waila-icon.png",
    "scheme": "waila",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/splash-ios.png",
        "tabletImage": "./assets/splash-ios.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFFFFF"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/waila-icon.png"
      },
      "edgeToEdgeEnabled": false,
      "splash": {
        "image": "./assets/splash-android.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Разрешить доступ к камере?",
          "microphonePermission": "Разрешить доступ к микрофону?"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Разрешить доступ к галерее?",
          "savePhotosPermission": "Разрешить сохранять фото?"
        }
      ],
      "expo-splash-screen"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9831b10b-fa19-4034-b2d5-3796ffd77f46"
      }
    }
  }
}
